{
  "name": "livescript_project_example",
  "version": "0.1.0",
  "description": "a simple template of how you can setup your package.json file",
  "main": "./lib/main.js",
  "scripts": {
    "cc": "./node_modules/.bin/lsc -o lib -c src/*.ls",
    "test-cc": "./node_modules/.bin/lsc -o lib-test -c test/*.ls",
    "pretest": "npm run-script cc && npm run-script test-cc",
    "test": "./node_modules/.bin/mocha lib-test/*.js",
    "prepublish": "npm run-script cc"
  },
  "keywords": [
    "livescript",
    "bootstrap"
  ],
  "author": "seanstrom",
  "license": "ISC",
  "devDependencies": {
    "LiveScript": "^1.2.0",
    "chai": "^1.9.1",
    "mocha": "^1.21.4"
  }
}
